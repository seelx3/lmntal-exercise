/* 	Welcome to AtCoder
output:
- sum of a, b, c
- string s
*/

main.

main :-
  a=integer.of_str(io.read_token(io.stdin,I1)),
  b=integer.of_str(io.read_token(I1,I2)),
  c=integer.of_str(io.read_token(I2,I3)),
  s=io.read_token(I3,io.free_port),
  l1.

l1, a(A), b(B), c(C) :-
  S = A+B+C |
  io.print(io.stdout,string.from(S),io.free_port),
  l2.

l2, s(S) :- 
  io.print(io.stdout," ",O),
  io.print_line(O,S,io.free_port).

